{
	"name": "Ruby, Go, and MySQL",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",

	"settings": { 
		"terminal.integrated.shell.linux": "/bin/bash"
	},

	"extensions": [
		"golang.go",
		"rebornix.Ruby"
	],

	"postCreateCommand": "bash /workspace/scripts/install-dev-tools.sh",
}